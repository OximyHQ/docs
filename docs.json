{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "linden",
  "name": "Oximy",
  "logo": {
    "dark": "/logo/Oximy.svg",
    "light": "/logo/Oximy.svg"
  },
  "favicon": "/favicon.ico",
  "colors": {
    "primary": "#FF4D01",
    "light": "#FF4D01",
    "dark": "#FF4D01"
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "introduction",
              "quickstart"
            ]
          },
          {
            "group": "Sensor",
            "pages": [
              "sensor/installation",
              "sensor/settings",
              "sensor/enrollment"
            ]
          },
          {
            "group": "Dashboard",
            "pages": [
              "dashboard/overview",
              "dashboard/ask"
            ]
          },
          {
            "group": "Support",
            "pages": [
              "troubleshooting"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Dashboard",
          "href": "https://app.oximy.com",
          "icon": "grid-2"
        },
        {
          "anchor": "GitHub",
          "href": "https://github.com/oximyhq/sensor",
          "icon": "github"
        }
      ]
    }
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@oximy.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Get Started",
      "href": "https://app.oximy.com/sign-up"
    }
  },
  "footer": {
    "socials": {
      "github": "https://github.com/oximyhq"
    }
  }
